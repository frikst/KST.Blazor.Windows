<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>KST.Blazor.Windows.Abstractions</RootNamespace>
    <AssemblyName>KST.Blazor.Windows.Abstractions</AssemblyName>
    <LangVersion>9.0</LangVersion>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>KST.Blazor.Windows.Abstractions</PackageId>
    <Authors>Jan Janech</Authors>
    <Company>Department of Software Technologies, Faculty of Management Science and Informatics, University of Žilina</Company>
    <Product>KST.Blazor.Windows.Abstractions</Product>
    <Description>Library adding possibility to open Blazor components in a new window.</Description>
    <Copyright>© 2021 Department of Software Technologies</Copyright>
    <PackageLicenseExpression>LGPL-3.0-or-later</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/frikst/KST.Blazor.Windows</PackageProjectUrl>
    <Version>1.0.0-pre20211216</Version>
    <AssemblyVersion>0.9.9</AssemblyVersion>
    <FileVersion>0.9.9</FileVersion>
  </PropertyGroup>

  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <PropertyGroup>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components" Version="5.0.12" />
  </ItemGroup>

</Project>
